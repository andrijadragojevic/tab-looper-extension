{
    "manifest_version": 3,
    "name": "Tab Looper - Automatically Cycle Opened Tabs",
    "version": "1.0",
    "description": "Automatically cycles and loops through all the opened tabs at a user-defined interval.",
    "permissions": ["tabs", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon-transparent.png"
    },
    "icons": {
      "16": "icon-transparent.png",
      "48": "icon-transparent.png",
      "128": "icon-transparent.png"
    }
}